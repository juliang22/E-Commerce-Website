{
  "name": "shopping-car-app",
  "version": "1.0.0",
  "description": "Practicing the mern stack woot woot",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm i && node server.js",
    "server": "nodemon server.js",
    "client": "cd ../eCommerce-frontend && npm start",
    "clean": "pkill node && lsof -i tcp:5000 | awk '{if(NR>1) print $2}' | xargs kill -9",
    "dev": "concurrently \" npm run server\" \" npm run client\"",
    "data:import": "node data/seeder",
    "data:destroy": "node data/seeder -d"
  },
  "author": "Julian Grunauer",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-capacitor": "^6.2.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.11.1"
  }
}